{
  "plugins": [
    "import",
    "standard",
    "promise",
    "node"
  ],
  "extends": [
  ],

  "parserOptions": {
    "ecmaVersion": 6,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": false
    }
  },
  "rules": {
    "semi": 2
  },
  "env": {
    "browser": true,
    "node": true
  }
}